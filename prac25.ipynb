   ],
   "source": [
    "def print_factors(x):\n",
    "    print(\"the factorss of \",x, \"are:\")\n",
    "    for i in range(1, x+1):\n",
    "        if x % i==0:\n",
    "            print(i)\n",
    "num= 240\n",
    "\n",
    "print_factors(num)"
   ]
